{
    "name": "my-inventory-app",
    "private": true,
    "scripts": {
        "check:code-style": "npm run check:code-style:prettier && npm run check:code-style:lint",
        "check:code-style:lint": "eslint .",
        "check:code-style:prettier": "prettier --check .",
        "check:type": "tsc --version && tsc --noEmit",
        "start:nasdb:run": "export MONGO_URL=\"${NAS_MONGO_URL}\" && npm start",
        "start:nasdb": "dotenv -e ../.env npm run start:nasdb:run",
        "start": "meteor run",
        "test": "meteor test --once --driver-package meteortesting:mocha",
        "test-app": "TEST_WATCH=1 meteor test --full-app --driver-package meteortesting:mocha",
        "visualize": "meteor --production --extra-packages bundle-visualizer"
    },
    "dependencies": {
        "@babel/core": "^7.28.4",
        "@babel/runtime": "^7.28.4",
        "lodash": "^4.17.21",
        "meteor-node-stubs": "^1.2.24",
        "polished": "^4.3.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "styled-components": "^5.3.11"
    },
    "devDependencies": {
        "@types/chai": "^5.2.2",
        "@types/lodash": "^4.17.20",
        "@types/meteor": "^2.9.9",
        "@types/mocha": "^10.0.10",
        "@types/mongodb": "^4.0.6",
        "@types/node": "^24.5.2",
        "@types/react": "^18.3.24",
        "@types/react-dom": "^18.3.7",
        "@types/sinon": "^17.0.4",
        "@types/styled-components": "^5.1.34",
        "@typescript-eslint/eslint-plugin": "^8.44.0",
        "@typescript-eslint/parser": "^8.44.0",
        "@xch/prettier-config": "^0.1.1",
        "babel-plugin-styled-components": "^2.1.4",
        "chai": "^6.0.1",
        "dotenv-cli": "^7.4.4",
        "eslint": "^8.57.1",
        "eslint-config-love": "^84.1.1",
        "eslint-config-prettier": "^8.10.2",
        "eslint-import-resolver-meteor": "^0.4.0",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-meteor": "^7.3.0",
        "eslint-plugin-n": "^17.23.0",
        "eslint-plugin-promise": "^7.2.1",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^5.2.0",
        "globals": "^16.4.0",
        "prettier": "2.8.8",
        "sinon": "^21.0.0",
        "typescript": "^5.9.2"
    },
    "engines": {
        "node": ">=22.0.0",
        "npm": ">=10.0.0"
    },
    "prettier": "@xch/prettier-config",
    "meteor": {
        "mainModule": {
            "client": "client/main.tsx",
            "server": "server/main.ts"
        }
    }
}
